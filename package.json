{
  "name": "vue3-admin-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": {
    "name": "tuyongtao",
    "email": "731156510@qq.com",
    "url": "http://tuyongtao.top/fettjob/"
  },
  "scripts": {
    "dev": "vite",
    "dev:pre": "vite --mode prepare",
    "dev:prod": "vite --mode production",
    "build:dev": "rimraf dist && vite build --mode development",
    "build:pre": "rimraf dist && vite build --mode prepare",
    "build": "rimraf dist && vite build --mode production",
    "report": "rimraf dist && vue-tsc && vite build",
    "preview": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "format": "prettier --write .",
    "check": "vue-tsc",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:style": "stylelint \"**/*.{vue,less,postcss,css,scss}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "commit"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.3.4",
    "echarts": "^5.5.1",
    "element-plus": "^2.8.7",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/prompt-cli": "^17.5.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/lodash-es": "^4.17.7",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "rimraf": "^4.4.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass-embedded": "^1.80.6",
    "stylelint": "^16.10.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.6.3",
    "unocss": "^0.64.0",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-fonts": "^1.0.3",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-define-options": "^1.2.4",
    "vite": "^5.4.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^3.0.2",
    "vue-tsc": "^2.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuyongtao-T/vue3-admin-ts.git"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{css,scss,less,style,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.18.6"
  },
  "volta": {
    "node": "16.18.1"
  }
}
